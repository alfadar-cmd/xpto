<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE eagle SYSTEM "eagle.dtd">
<eagle version="9.6.2">
<drawing>
<settings>
<setting alwaysvectorfont="yes"/>
<setting verticaltext="up"/>
</settings>

<library>
<description>
I2C Sensor Hub
1x I2C + Power IN (from Raspberry Pi)
3x I2C + Power OUT (to sensors)
Connectors: THT 2.54mm
Compatible with EAGLE 9.6.2
</description>

<!-- ================================================= -->
<!-- PACKAGE -->
<!-- ================================================= -->
<packages>
<package name="I2C_HUB_1IN_3OUT_THT_2P54">
<description>
Footprint:
- 1x 4-pin IN (5V, GND, SCL, SDA)
- 3x 4-pin OUT (VCC, GND, SCL, SDA)
All connectors THT 2.54mm
</description>

<!-- Board outline -->
<wire x1="-15" y1="-15" x2="15" y2="-15" width="0.127" layer="21"/>
<wire x1="15" y1="-15" x2="15" y2="15" width="0.127" layer="21"/>
<wire x1="15" y1="15" x2="-15" y2="15" width="0.127" layer="21"/>
<wire x1="-15" y1="15" x2="-15" y2="-15" width="0.127" layer="21"/>

<!-- IN connector (left) -->
<pad name="IN_5V"  x="-10.16" y="5.08"  drill="1.0" diameter="1.8" shape="square"/>
<pad name="IN_GND" x="-10.16" y="2.54"  drill="1.0" diameter="1.8"/>
<pad name="IN_SCL" x="-10.16" y="-2.54" drill="1.0" diameter="1.8"/>
<pad name="IN_SDA" x="-10.16" y="-5.08" drill="1.0" diameter="1.8"/>

<!-- OUT1 -->
<pad name="O1_5V"  x="10.16" y="7.62" drill="1.0" diameter="1.8" shape="square"/>
<pad name="O1_GND" x="10.16" y="5.08" drill="1.0" diameter="1.8"/>
<pad name="O1_SCL" x="10.16" y="2.54" drill="1.0" diameter="1.8"/>
<pad name="O1_SDA" x="10.16" y="0.00" drill="1.0" diameter="1.8"/>

<!-- OUT2 -->
<pad name="O2_5V"  x="10.16" y="-2.54" drill="1.0" diameter="1.8" shape="square"/>
<pad name="O2_GND" x="10.16" y="-5.08" drill="1.0" diameter="1.8"/>
<pad name="O2_SCL" x="10.16" y="-7.62" drill="1.0" diameter="1.8"/>
<pad name="O2_SDA" x="10.16" y="-10.16" drill="1.0" diameter="1.8"/>

<!-- OUT3 -->
<pad name="O3_5V"  x="10.16" y="-12.70" drill="1.0" diameter="1.8" shape="square"/>
<pad name="O3_GND" x="10.16" y="-15.24" drill="1.0" diameter="1.8"/>
<pad name="O3_SCL" x="10.16" y="-17.78" drill="1.0" diameter="1.8"/>
<pad name="O3_SDA" x="10.16" y="-20.32" drill="1.0" diameter="1.8"/>

<text x="-14" y="16.5" size="1.27" layer="25">&gt;NAME</text>
<text x="-14" y="-18" size="1.27" layer="27">&gt;VALUE</text>
</package>
</packages>

<!-- ================================================= -->
<!-- SYMBOL -->
<!-- ================================================= -->
<symbols>
<symbol name="I2C_SENSOR_HUB">
  <description>I2C Sensor Hub – 1 IN + 3 OUT (distributed pins)</description>

  <!-- Body -->
  <wire x1="-17.78" y1="-12.70" x2="17.78" y2="-12.70" width="0.254" layer="94"/>
  <wire x1="17.78"  y1="-12.70" x2="17.78"  y2="12.70"  width="0.254" layer="94"/>
  <wire x1="17.78"  y1="12.70"  x2="-17.78" y2="12.70"  width="0.254" layer="94"/>
  <wire x1="-17.78" y1="12.70"  x2="-17.78" y2="-12.70" width="0.254" layer="94"/>

  <!-- Name / Value -->
  <text x="-17.78" y="13.97" size="1.778" layer="95">&gt;NAME</text>
  <text x="-17.78" y="-15.24" size="1.778" layer="96">&gt;VALUE</text>

  <!-- ================= IN (left) ================= -->
  <pin name="5V_IN"  x="-22.86" y="5.08"  length="short" direction="pwr"/>
  <pin name="GND_IN" x="-22.86" y="2.54"  length="short" direction="pwr"/>
  <pin name="SCL"    x="-22.86" y="-2.54" length="short" direction="io"/>
  <pin name="SDA"    x="-22.86" y="-5.08" length="short" direction="io"/>

  <!-- ================= OUT 1 (right) ================= -->
  <pin name="5V_1"  x="22.86" y="5.08" length="short" direction="pwr" rot="R180"/>
  <pin name="GND_1" x="22.86" y="2.54" length="short" direction="pwr" rot="R180"/>
  <pin name="SCL_1" x="22.86" y="-2.54" length="short" direction="io"  rot="R180"/>
  <pin name="SDA_1" x="22.86" y="-5.08" length="short" direction="io"  rot="R180"/>

  <!-- ================= OUT 2 (top) ================= -->
  <pin name="5V_2"  x="-5.08" y="17.78" length="short" direction="pwr" rot="R270"/>
  <pin name="GND_2" x="-2.54" y="17.78" length="short" direction="pwr" rot="R270"/>
  <pin name="SCL_2" x="2.54"  y="17.78" length="short" direction="io"  rot="R270"/>
  <pin name="SDA_2" x="5.08"  y="17.78" length="short" direction="io"  rot="R270"/>

  <!-- ================= OUT 3 (bottom) ================= -->
  <pin name="5V_3"  x="-5.08" y="-17.78" length="short" direction="pwr" rot="R90"/>
  <pin name="GND_3" x="-2.54" y="-17.78" length="short" direction="pwr" rot="R90"/>
  <pin name="SCL_3" x="2.54"  y="-17.78" length="short" direction="io"  rot="R90"/>
  <pin name="SDA_3" x="5.08"  y="-17.78" length="short" direction="io"  rot="R90"/>

  <!-- Label -->
  <text x="-8.89" y="-1.27" size="1.27" layer="94">
Hub I2C de Sensores
  </text>
</symbol>
</symbols>

<!-- ================================================= -->
<!-- DEVICESET -->
<!-- ================================================= -->
<devicesets>
<deviceset name="I2C_SENSOR_HUB_1IN_3OUT" prefix="U">
<description>I2C Sensor Hub – powered by Raspberry Pi</description>

<gates>
<gate name="G$1" symbol="I2C_SENSOR_HUB" x="0" y="0"/>
</gates>

<devices>
<device name="" package="I2C_HUB_1IN_3OUT_THT_2P54">
<connects>
<connect gate="G$1" pin="5V_IN"  pad="IN_5V"/>
<connect gate="G$1" pin="GND_IN" pad="IN_GND"/>
<connect gate="G$1" pin="SCL"    pad="IN_SCL"/>
<connect gate="G$1" pin="SDA"    pad="IN_SDA"/>

<connect gate="G$1" pin="5V_1"  pad="O1_5V"/>
<connect gate="G$1" pin="GND_1" pad="O1_GND"/>
<connect gate="G$1" pin="SCL_1" pad="O1_SCL"/>
<connect gate="G$1" pin="SDA_1" pad="O1_SDA"/>

<connect gate="G$1" pin="5V_2"  pad="O2_5V"/>
<connect gate="G$1" pin="GND_2" pad="O2_GND"/>
<connect gate="G$1" pin="SCL_2" pad="O2_SCL"/>
<connect gate="G$1" pin="SDA_2" pad="O2_SDA"/>

<connect gate="G$1" pin="5V_3"  pad="O3_5V"/>
<connect gate="G$1" pin="GND_3" pad="O3_GND"/>
<connect gate="G$1" pin="SCL_3" pad="O3_SCL"/>
<connect gate="G$1" pin="SDA_3" pad="O3_SDA"/>
</connects>

<technologies>
<technology name=""/>
</technologies>
</device>
</devices>
</deviceset>
</devicesets>

</library>
</drawing>
</eagle>
