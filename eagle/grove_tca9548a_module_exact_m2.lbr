                <connect gate="G$1" pin="SD1" pad=""/>
                <connect gate="G$1" pin="SC2" pad=""/>
                <connect gate="G$1" pin="SD2" pad=""/>
                <connect gate="G$1" pin="SC3" pad=""/>
                <connect gate="G$1" pin="SD3" pad=""/>
                <connect gate="G$1" pin="SC4" pad=""/>
                <connect gate="G$1" pin="SD4" pad=""/>
                <connect gate="G$1" pin="SC5" pad=""/>
