<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE eagle SYSTEM "eagle.dtd">
<eagle version="9.6.2">
  <drawing>
    <settings>
      <setting alwaysvectorfont="yes"/>
      <setting verticaltext="up"/>
    </settings>

    <library>
      <description>
Grove TCA9548A I2C Hub - footprint exacto (82x22mm) + furos M2 + keepout
Inclui:
1) MECH_ONLY: apenas mecânico (recomendado se o módulo é externo)
2) I2C_MIN: símbolo mínimo (VCC/GND/SCL/SDA) para documentação
      </description>

      <packages>
        <package name="GROVE_TCA9548A_MODULE_82X22_M2_5H">
          <description>Outline 82x22mm + 5 furos M2 (Ø2.0mm) + keepout. Origem no centro.</description>

          <!-- Outline -->
          <wire x1="-41" y1="-11" x2="41" y2="-11" width="0.127" layer="21"/>
          <wire x1="41" y1="-11" x2="41" y2="11" width="0.127" layer="21"/>
          <wire x1="41" y1="11" x2="-41" y2="11" width="0.127" layer="21"/>
          <wire x1="-41" y1="11" x2="-41" y2="-11" width="0.127" layer="21"/>

          <!-- Keepout -->
          <wire x1="-41" y1="-11" x2="41" y2="-11" width="0.127" layer="39"/>
          <wire x1="41" y1="-11" x2="41" y2="11" width="0.127" layer="39"/>
          <wire x1="41" y1="11" x2="-41" y2="11" width="0.127" layer="39"/>
          <wire x1="-41" y1="11" x2="-41" y2="-11" width="0.127" layer="39"/>

          <!-- Furos M2 (Ø2.0mm) -->
          <hole x="-31.10" y="9.34" drill="2.0"/>
          <hole x="-31.41" y="-10.93" drill="2.0"/>
          <hole x="9.14"  y="9.19" drill="2.0"/>
          <hole x="9.14"  y="-11.08" drill="2.0"/>
          <hole x="39.08" y="-1.10" drill="2.0"/>

          <!-- Dummy pads (só para o deviceset I2C_MIN; ficam fora do contorno) -->
          <smd name="P_VCC" x="45" y="5" dx="0.5" dy="0.5" layer="1"/>
          <smd name="P_GND" x="45" y="4" dx="0.5" dy="0.5" layer="1"/>
          <smd name="P_SCL" x="45" y="3" dx="0.5" dy="0.5" layer="1"/>
          <smd name="P_SDA" x="45" y="2" dx="0.5" dy="0.5" layer="1"/>

          <text x="-41" y="13" size="1.27" layer="25">&gt;NAME</text>
          <text x="-41" y="-14.5" size="1.27" layer="27">&gt;VALUE</text>
        </package>
      </packages>

      <symbols>
        <!-- Símbolo mecânico (sem pinos) -->
        <symbol name="GROVE_TCA9548A_MECH">
          <description>Representação mecânica (sem ligações eléctricas).</description>
          <wire x1="-20.32" y1="-5.08" x2="20.32" y2="-5.08" width="0.254" layer="94"/>
          <wire x1="20.32" y1="-5.08" x2="20.32" y2="5.08" width="0.254" layer="94"/>
          <wire x1="20.32" y1="5.08" x2="-20.32" y2="5.08" width="0.254" layer="94"/>
          <wire x1="-20.32" y1="5.08" x2="-20.32" y2="-5.08" width="0.254" layer="94"/>
          <text x="-20.32" y="6.35" size="1.778" layer="95">&gt;NAME</text>
          <text x="-20.32" y="-7.62" size="1.778" layer="96">&gt;VALUE</text>
          <text x="-17.78" y="-1.27" size="1.778" layer="94">Grove TCA9548A</text>
          <text x="-17.78" y="-3.81" size="1.27" layer="94">MECH ONLY</text>
        </symbol>

        <!-- Símbolo mínimo (só I2C + power) -->
        <symbol name="GROVE_TCA9548A_I2C_MIN">
          <description>Ligação mínima: VCC, GND, SCL, SDA.</description>
          <wire x1="-17.78" y1="-7.62" x2="17.78" y2="-7.62" width="0.254" layer="94"/>
          <wire x1="17.78" y1="-7.62" x2="17.78" y2="7.62" width="0.254" layer="94"/>
          <wire x1="17.78" y1="7.62" x2="-17.78" y2="7.62" width="0.254" layer="94"/>
          <wire x1="-17.78" y1="7.62" x2="-17.78" y2="-7.62" width="0.254" layer="94"/>
          <text x="-17.78" y="8.89" size="1.778" layer="95">&gt;NAME</text>
          <text x="-17.78" y="-9.91" size="1.778" layer="96">&gt;VALUE</text>

          <pin name="VCC" x="-22.86" y="2.54" length="short" direction="pwr"/>
          <pin name="GND" x="-22.86" y="0.00" length="short" direction="pwr"/>
          <pin name="SCL" x="-22.86" y="-2.54" length="short" direction="io"/>
          <pin name="SDA" x="-22.86" y="-5.08" length="short" direction="io"/>

          <text x="-15.24" y="4.57" size="1.27" layer="94">I2C HUB (min)</text>
        </symbol>
      </symbols>

      <devicesets>
        <!-- 1) Mecânico apenas (recomendado no teu caso) -->
        <deviceset name="GROVE_TCA9548A_MODULE_MECH_ONLY" prefix="M">
          <description>Coloca na PCB para contorno + furos + keepout. Sem ligações eléctricas.</description>
          <gates>
            <gate name="G$1" symbol="GROVE_TCA9548A_MECH" x="0" y="0"/>
          </gates>
          <devices>
            <device name="" package="GROVE_TCA9548A_MODULE_82X22_M2_5H">
              <connects/>
              <technologies><technology name=""/></technologies>
            </device>
          </devices>
        </deviceset>

        <!-- 2) Símbolo mínimo (só para documentação) -->
        <deviceset name="GROVE_TCA9548A_MODULE_I2C_MIN" prefix="U">
          <description>Só VCC/GND/SCL/SDA (para documentação). Usa pads dummy fora do contorno.</description>
          <gates>
            <gate name="G$1" symbol="GROVE_TCA9548A_I2C_MIN" x="0" y="0"/>
          </gates>
          <devices>
            <device name="" package="GROVE_TCA9548A_MODULE_82X22_M2_5H">
              <connects>
                <connect gate="G$1" pin="VCC" pad="P_VCC"/>
                <connect gate="G$1" pin="GND" pad="P_GND"/>
                <connect gate="G$1" pin="SCL" pad="P_SCL"/>
                <connect gate="G$1" pin="SDA" pad="P_SDA"/>
              </connects>
              <technologies><technology name=""/></technologies>
            </device>
          </devices>
        </deviceset>

      </devicesets>
    </library>
  </drawing>
</eagle>
