<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE eagle SYSTEM "eagle.dtd">
<eagle version="9.6.2">
  <drawing>
    <settings>
      <setting alwaysvectorfont="yes"/>
      <setting verticaltext="up"/>
    </settings>

    <library>
      <description>
Grove - 8 Channel I2C Multiplexer / I2C Hub (TCA9548A)
Module footprint (board outline 82x22mm) + mounting holes M2 (Ø2.0mm) + keepout
Holes extracted from Seeed scale image (top-left, bottom-left, top-middle, bottom-middle, right-side).
Includes: symbol + package + device (no electrical pads on footprint).
      </description>

      <packages>
        <package name="GROVE_TCA9548A_MODULE_82X22_M2_5H">
          <description>
Module outline 82x22mm, 5x mounting holes Ø2.0mm (M2), keepout.
Origin at board centre (0,0).
          </description>

          <!-- Board outline (silk) -->
          <wire x1="-41" y1="-11" x2="41" y2="-11" width="0.127" layer="21"/>
          <wire x1="41" y1="-11" x2="41" y2="11" width="0.127" layer="21"/>
          <wire x1="41" y1="11" x2="-41" y2="11" width="0.127" layer="21"/>
          <wire x1="-41" y1="11" x2="-41" y2="-11" width="0.127" layer="21"/>

          <!-- Keepout area to reserve space -->
          <wire x1="-41" y1="-11" x2="41" y2="-11" width="0.127" layer="39"/>
          <wire x1="41" y1="-11" x2="41" y2="11" width="0.127" layer="39"/>
          <wire x1="41" y1="11" x2="-41" y2="11" width="0.127" layer="39"/>
          <wire x1="-41" y1="11" x2="-41" y2="-11" width="0.127" layer="39"/>

          <!-- Mounting holes (NPTH) Ø2.0mm (M2) -->
          <!-- Coordinates in mm, origin at board centre -->
          <hole x="-31.10" y="9.34" drill="2.0"/>   <!-- top-left -->
          <hole x="-31.41" y="-10.93" drill="2.0"/> <!-- bottom-left -->
          <hole x="9.14"  y="9.19" drill="2.0"/>    <!-- top-middle -->
          <hole x="9.14"  y="-11.08" drill="2.0"/>  <!-- bottom-middle -->
          <hole x="39.08" y="-1.10" drill="2.0"/>   <!-- right-side -->

          <text x="-41" y="13" size="1.27" layer="25">&gt;NAME</text>
          <text x="-41" y="-14.5" size="1.27" layer="27">&gt;VALUE</text>
        </package>
      </packages>

      <symbols>
        <symbol name="GROVE_TCA9548A_HUB_8CH">
          <description>
Grove 8-Channel I2C Hub / Multiplexer (TCA9548A).
Upstream: VCC, GND, SCL_IN, SDA_IN, RST(optional)
Channels: SC0/SD0 ... SC7/SD7
          </description>

          <wire x1="-27.94" y1="-20.32" x2="27.94" y2="-20.32" width="0.254" layer="94"/>
          <wire x1="27.94" y1="-20.32" x2="27.94" y2="25.4" width="0.254" layer="94"/>
          <wire x1="27.94" y1="25.4" x2="-27.94" y2="25.4" width="0.254" layer="94"/>
          <wire x1="-27.94" y1="25.4" x2="-27.94" y2="-20.32" width="0.254" layer="94"/>

          <text x="-27.94" y="26.67" size="1.778" layer="95">&gt;NAME</text>
          <text x="-27.94" y="-22.86" size="1.778" layer="96">&gt;VALUE</text>

          <!-- Upstream -->
          <pin name="VCC"    x="-33.02" y="20.32" length="short" direction="pwr"/>
          <pin name="GND"    x="-33.02" y="17.78" length="short" direction="pwr"/>
          <pin name="SCL_IN" x="-33.02" y="12.70" length="short" direction="io"/>
          <pin name="SDA_IN" x="-33.02" y="10.16" length="short" direction="io"/>
          <pin name="RST"    x="-33.02" y="5.08"  length="short" direction="in"/>

          <!-- Channels -->
          <pin name="SC0" x="33.02" y="20.32" length="short" direction="io" rot="R180"/>
          <pin name="SD0" x="33.02" y="17.78" length="short" direction="io" rot="R180"/>
          <pin name="SC1" x="33.02" y="15.24" length="short" direction="io" rot="R180"/>
          <pin name="SD1" x="33.02" y="12.70" length="short" direction="io" rot="R180"/>
          <pin name="SC2" x="33.02" y="10.16" length="short" direction="io" rot="R180"/>
          <pin name="SD2" x="33.02" y="7.62"  length="short" direction="io" rot="R180"/>
          <pin name="SC3" x="33.02" y="5.08"  length="short" direction="io" rot="R180"/>
          <pin name="SD3" x="33.02" y="2.54"  length="short" direction="io" rot="R180"/>
          <pin name="SC4" x="33.02" y="0.00"  length="short" direction="io" rot="R180"/>
          <pin name="SD4" x="33.02" y="-2.54" length="short" direction="io" rot="R180"/>
          <pin name="SC5" x="33.02" y="-5.08" length="short" direction="io" rot="R180"/>
          <pin name="SD5" x="33.02" y="-7.62" length="short" direction="io" rot="R180"/>
          <pin name="SC6" x="33.02" y="-10.16" length="short" direction="io" rot="R180"/>
          <pin name="SD6" x="33.02" y="-12.70" length="short" direction="io" rot="R180"/>
          <pin name="SC7" x="33.02" y="-15.24" length="short" direction="io" rot="R180"/>
          <pin name="SD7" x="33.02" y="-17.78" length="short" direction="io" rot="R180"/>
        </symbol>
      </symbols>

      <devicesets>
        <deviceset name="GROVE_TCA9548A_MODULE_M2" prefix="U">
          <description>
Grove TCA9548A 8CH I2C Hub module with PCB footprint (82x22mm) and M2 holes.
Footprint has no electrical pads (module connectors are on the module itself).
          </description>

          <gates>
            <gate name="G$1" symbol="GROVE_TCA9548A_HUB_8CH" x="0" y="0"/>
          </gates>

          <devices>
            <device name="" package="GROVE_TCA9548A_MODULE_82X22_M2_5H">
              <connects>
                <connect gate="G$1" pin="VCC" pad=""/>
