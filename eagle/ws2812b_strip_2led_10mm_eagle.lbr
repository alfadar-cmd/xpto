<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE eagle SYSTEM "eagle.dtd">
<eagle version="9.6.2">
  <drawing>
    <settings>
      <setting alwaysvectorfont="yes"/>
      <setting verticaltext="up"/>
    </settings>

    <library>
      <description>
WS2812B / NeoPixel LED strip segment - 2 LEDs
Based on 60 LED/m, 5V, 10mm FPC width.
Segment length: 33.33mm (2 x 16.67mm).
Pads order per wiring diagram: GND / DATA / 5V.
Includes IN + OUT pads for chaining.
      </description>

      <packages>
        <package name="WS2812B_STRIP_2LED_10MM_INOUT_PADS">
          <description>
10mm wide strip segment footprint (2 LEDs @ 60 LED/m => 33.33mm length).
IN pads on left, OUT pads on right.
Pad order top-to-bottom: GND, DATA, 5V.
SMD pad size: 3.2 x 2.0mm (generic, robust for soldering wires).
          </description>

          <!-- Keepout / outline of the strip segment -->
          <!-- Origin at centre (0,0). Width 10mm => y = +/-5. Length 33.33mm => x = +/-16.665 -->
          <wire x1="-16.665" y1="-5" x2="16.665" y2="-5" width="0.127" layer="39"/>
          <wire x1="16.665" y1="-5" x2="16.665" y2="5" width="0.127" layer="39"/>
          <wire x1="16.665" y1="5" x2="-16.665" y2="5" width="0.127" layer="39"/>
          <wire x1="-16.665" y1="5" x2="-16.665" y2="-5" width="0.127" layer="39"/>

          <!-- Optional silkscreen outline -->
          <wire x1="-16.665" y1="-5" x2="16.665" y2="-5" width="0.127" layer="21"/>
          <wire x1="16.665" y1="-5" x2="16.665" y2="5" width="0.127" layer="21"/>
          <wire x1="16.665" y1="5" x2="-16.665" y2="5" width="0.127" layer="21"/>
          <wire x1="-16.665" y1="5" x2="-16.665" y2="-5" width="0.127" layer="21"/>

          <!-- IN pads (left) -->
          <!-- Order top-to-bottom: GND, DATA, 5V -->
          <smd name="GND_IN" x="-14.5" y="2.54"  dx="3.2" dy="2.0" layer="1"/>
          <smd name="DIN"    x="-14.5" y="0.00"  dx="3.2" dy="2.0" layer="1"/>
          <smd name="5V_IN"  x="-14.5" y="-2.54" dx="3.2" dy="2.0" layer="1"/>

          <!-- OUT pads (right) -->
          <!-- Order top-to-bottom: GND, DATA, 5V -->
          <smd name="GND_OUT" x="14.5" y="2.54"  dx="3.2" dy="2.0" layer="1"/>
          <smd name="DOUT"    x="14.5" y="0.00"  dx="3.2" dy="2.0" layer="1"/>
          <smd name="5V_OUT"  x="14.5" y="-2.54" dx="3.2" dy="2.0" layer="1"/>

          <!-- Labels -->
          <text x="-16.2" y="4.2" size="1.27" layer="21">IN</text>
          <text x="13.2"  y="4.2" size="1.27" layer="21">OUT</text>

          <text x="-16.665" y="-7.8" size="1.27" layer="25">&gt;NAME</text>
          <text x="-16.665" y="-9.6" size="1.27" layer="27">&gt;VALUE</text>
        </package>
      </packages>

      <symbols>
        <symbol name="WS2812B_STRIP_2LED">
          <description>WS2812B strip segment (2 LEDs). DIN -> DOUT chain.</description>

          <wire x1="-20.32" y1="-10.16" x2="20.32" y2="-10.16" width="0.254" layer="94"/>
          <wire x1="20.32" y1="-10.16" x2="20.32" y2="10.16" width="0.254" layer="94"/>
          <wire x1="20.32" y1="10.16" x2="-20.32" y2="10.16" width="0.254" layer="94"/>
          <wire x1="-20.32" y1="10.16" x2="-20.32" y2="-10.16" width="0.254" layer="94"/>

          <text x="-20.32" y="11.43" size="1.778" layer="95">&gt;NAME</text>
          <text x="-20.32" y="-12.70" size="1.778" layer="96">&gt;VALUE</text>

          <!-- IN (left) -->
          <pin name="5V"  x="-25.40" y="5.08"  length="short" direction="pwr"/>
          <pin name="GND" x="-25.40" y="0.00"  length="short" direction="pwr"/>
          <pin name="DIN" x="-25.40" y="-5.08" length="short" direction="in"/>

          <!-- OUT (right) -->
          <pin name="5V_OUT"  x="25.40" y="5.08"  length="short" direction="pwr" rot="R180"/>
          <pin name="GND_OUT" x="25.40" y="0.00"  length="short" direction="pwr" rot="R180"/>
          <pin name="DOUT"    x="25.40" y="-5.08" length="short" direction="out" rot="R180"/>

          <text x="-17.78" y="7.62" size="1.27" layer="94">WS2812B strip (2 LEDs)</text>
        </symbol>
      </symbols>

      <devicesets>
        <deviceset name="WS2812B_STRIP_2LED_10MM" prefix="LED">
          <description>WS2812B strip segment, 2 LEDs, 10mm width, IN/OUT pads.</description>

          <gates>
            <gate name="G$1" symbol="WS2812B_STRIP_2LED" x="0" y="0"/>
          </gates>

          <devices>
            <device name="" package="WS2812B_STRIP_2LED_10MM_INOUT_PADS">
              <connects>
                <connect gate="G$1" pin="5V"      pad="5V_IN"/>
                <connect gate="G$1" pin="GND"     pad="GND_IN"/>
                <connect gate="G$1" pin="DIN"     pad="DIN"/>
                <connect gate="G$1" pin="5V_OUT"  pad="5V_OUT"/>
                <connect gate="G$1" pin="GND_OUT" pad="GND_OUT"/>
                <connect gate="G$1" pin="DOUT"    pad="DOUT"/>
              </connects>

              <technologies>
                <technology name=""/>
              </technologies>
            </device>
          </devices>
        </deviceset>
      </devicesets>

    </library>
  </drawing>
</eagle>
