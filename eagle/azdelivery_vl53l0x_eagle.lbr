    <library>
      <description>
AZ-Delivery VL53L0X Time-of-Flight (ToF) Distance Sensor Module
Pinout (top-to-bottom as in AZ-Delivery image): VCC, GND, SCL, SDA, GPIO1, XSHUT
Includes: symbol + 1x6 2.54mm THT header footprint + device mapping
      </description>

      <packages>
        <package name="AZDELIVERY_VL53L0X_HDR_1X6_2P54_THT">
          <description>AZ-Delivery VL53L0X module - 1x6 header, 2.54mm, THT. Pin 1 = VCC.</description>

          <!-- Silk outline (generic) -->
          <wire x1="-2.54" y1="-2.54" x2="2.54" y2="-2.54" width="0.127" layer="21"/>
          <wire x1="2.54" y1="-2.54" x2="2.54" y2="15.24" width="0.127" layer="21"/>
          <wire x1="2.54" y1="15.24" x2="-2.54" y2="15.24" width="0.127" layer="21"/>
          <wire x1="-2.54" y1="15.24" x2="-2.54" y2="-2.54" width="0.127" layer="21"/>

          <!-- Pin 1 marker -->
          <text x="-2.54" y="15.875" size="1.27" layer="21">1</text>

          <!-- Pads: 1x6 vertical, pitch 2.54mm -->
          <!-- Pad 1 at top (y=12.70), pad 6 at bottom (y=0.00) -->
          <pad name="1" x="0" y="12.70" drill="1.0" diameter="1.8" shape="square"/>
          <pad name="2" x="0" y="10.16" drill="1.0" diameter="1.8"/>
          <pad name="3" x="0" y="7.62"  drill="1.0" diameter="1.8"/>
          <pad name="4" x="0" y="5.08"  drill="1.0" diameter="1.8"/>
          <pad name="5" x="0" y="2.54"  drill="1.0" diameter="1.8"/>
          <pad name="6" x="0" y="0.00"  drill="1.0" diameter="1.8"/>

          <text x="-2.54" y="-4.318" size="1.27" layer="25">&gt;NAME</text>
          <text x="-2.54" y="-6.096" size="1.27" layer="27">&gt;VALUE</text>
        </package>
      </packages>

      <symbols>
        <symbol name="AZDELIVERY_VL53L0X">
          <description>
AZ-Delivery VL53L0X ToF module (I2C).
Pinout (top-to-bottom as in module image): VCC, GND, SCL, SDA, GPIO1, XSHUT
Notes: I2C default address often 0x29 (depends on library/config).
          </description>

          <wire x1="-17.78" y1="-7.62" x2="17.78" y2="-7.62" width="0.254" layer="94"/>
          <wire x1="17.78" y1="-7.62" x2="17.78" y2="12.70" width="0.254" layer="94"/>
          <wire x1="17.78" y1="12.70" x2="-17.78" y2="12.70" width="0.254" layer="94"/>
