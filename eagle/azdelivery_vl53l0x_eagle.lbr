<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE eagle SYSTEM "eagle.dtd">
<eagle version="9.6.2">
<drawing>
<settings>
<setting alwaysvectorfont="yes"/>
<setting verticaltext="up"/>
</settings>

<library>
<description>
AZDelivery VL53L0X Time-of-Flight Distance Sensor Module
I2C interface (VCC, GND, SCL, SDA)
System-level symbol + simple pad footprint
Compatible with EAGLE 9.6.2
</description>

<!-- ================================================= -->
<!-- PACKAGE -->
<!-- ================================================= -->
<packages>
<package name="AZDELIVERY_VL53L0X_PADS_4P_2P54">
<description>
4-pin THT pads for AZDelivery VL53L0X module
Pin order: VCC, GND, SCL, SDA
2.54mm pitch â€“ for wires or pin headers
</description>

<!-- Pads -->
<pad name="VCC" x="0" y="3.81" drill="1.0" diameter="1.8" shape="square"/>
<pad name="GND" x="0" y="1.27" drill="1.0" diameter="1.8"/>
<pad name="SCL" x="0" y="-1.27" drill="1.0" diameter="1.8"/>
<pad name="SDA" x="0" y="-3.81" drill="1.0" diameter="1.8"/>

<!-- Silk outline -->
<wire x1="-3.5" y1="6" x2="3.5" y2="6" width="0.127" layer="21"/>
<wire x1="3.5" y1="6" x2="3.5" y2="-6" width="0.127" layer="21"/>
<wire x1="3.5" y1="-6" x2="-3.5" y2="-6" width="0.127" layer="21"/>
<wire x1="-3.5" y1="-6" x2="-3.5" y2="6" width="0.127" layer="21"/>

<!-- Labels -->
<text x="-6.5" y="3.4" size="1.27" layer="21">VCC</text>
<text x="-6.5" y="0.9" size="1.27" layer="21">GND</text>
<text x="-6.5" y="-1.6" size="1.27" layer="21">SCL</text>
<text x="-6.5" y="-4.1" size="1.27" layer="21">SDA</text>

<text x="-5.5" y="6.8" size="1.27" layer="25">&gt;NAME</text>
<text x="-5.5" y="-8.5" size="1.27" layer="27">&gt;VALUE</text>
</package>
</packages>

<!-- ================================================= -->
<!-- SYMBOL -->
<!-- ================================================= -->
<symbols>
<symbol name="VL53L0X_I2C_SENSOR">
<description>VL53L0X Time-of-Flight Distance Sensor (I2C)</description>

<wire x1="-10.16" y1="-7.62" x2="10.16" y2="-7.62" width="0.254" layer="94"/>
<wire x1="10.16" y1="-7.62" x2="10.16" y2="7.62" width="0.254" layer="94"/>
<wire x1="10.16" y1="7.62" x2="-10.16" y2="7.62" width="0.254" layer="94"/>
<wire x1="-10.16" y1="7.62" x2="-10.16" y2="-7.62" width="0.254" layer="94"/>

<text x="-10.16" y="8.89" size="1.778" layer="95">&gt;NAME</text>
<text x="-10.16" y="-10.16" size="1.778" layer="96">&gt;VALUE</text>

<!-- Pins -->
<pin name="VCC" x="-15.24" y="2.54" length="short" direction="pwr"/>
<pin name="GND" x="-15.24" y="0.00" length="short" direction="pwr"/>
<pin name="SCL" x="-15.24" y="-2.54" length="short" direction="io"/>
<pin name="SDA" x="-15.24" y="-5.08" length="short" direction="io"/>

<text x="-7.62" y="4.06" size="1.27" layer="94">VL53L0X</text>
<text x="-7.62" y="2.54" size="1.27" layer="94">ToF Sensor</text>
</symbol>
</symbols>

<!-- ================================================= -->
<!-- DEVICESET -->
<!-- ================================================= -->
<devicesets>
<deviceset name="AZDELIVERY_VL53L0X" prefix="U">
<description>AZDelivery VL53L0X I2C Time-of-Flight Distance Sensor Module</description>

<gates>
<gate name="G$1" symbol="VL53L0X_I2C_SENSOR" x="0" y="0"/>
</gates>

<devices>
<device name="" package="AZDELIVERY_VL53L0X_PADS_4P_2P54">
<connects>
<connect gate="G$1" pin="VCC" pad="VCC"/>
<connect gate="G$1" pin="GND" pad="GND"/>
<connect gate="G$1" pin="SCL" pad="SCL"/>
<connect gate="G$1" pin="SDA" pad="SDA"/>
</connects>

<technologies>
<technology name=""/>
</technologies>
</device>
</devices>
</deviceset>
</devicesets>

</library>
</drawing>
</eagle>
