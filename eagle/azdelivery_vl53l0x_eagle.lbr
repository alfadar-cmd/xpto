          <text x="-17.78" y="13.97" size="1.778" layer="95">&gt;NAME</text>
          <text x="-17.78" y="-9.398" size="1.778" layer="96">&gt;VALUE</text>

          <!-- Pins on left for clean wiring -->
          <pin name="VCC"   x="-22.86" y="10.16" length="short" direction="pwr"/>
          <pin name="GND"   x="-22.86" y="7.62"  length="short" direction="pwr"/>
          <pin name="SCL"   x="-22.86" y="5.08"  length="short" direction="io"/>
          <pin name="SDA"   x="-22.86" y="2.54"  length="short" direction="io"/>
          <pin name="GPIO1" x="-22.86" y="0.00"  length="short" direction="io"/>
          <pin name="XSHUT" x="-22.86" y="-2.54" length="short" direction="in"/>

          <text x="-15.24" y="10.16" size="1.27" layer="94">Power</text>
          <text x="-15.24" y="4.318"  size="1.27" layer="94">I2C</te
