<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE eagle SYSTEM "eagle.dtd">
<eagle version="9.6.2">
<drawing>
<settings>
<setting alwaysvectorfont="yes"/>
<setting verticaltext="up"/>
</settings>

<library>
<description>
WS2812 / NeoPixel single LED
THT version with 2.54mm pins
Pins: 5V, DIN, GND, DOUT
Compatible with EAGLE 9
</description>

<!-- ================================================= -->
<!-- PACKAGE -->
<!-- ================================================= -->
<packages>
<package name="WS2812_THT_2P54">
<description>WS2812 single LED – 4x THT pads, 2.54mm pitch</description>

<!-- Pads: left column = IN, right = OUT -->
<!-- Drill 1.0mm, good for wires or pin headers -->

<pad name="5V"  x="0"    y="2.54" drill="1.0" diameter="1.8" shape="square"/>
<pad name="DIN" x="0"    y="0.00" drill="1.0" diameter="1.8"/>
<pad name="GND" x="0"    y="-2.54" drill="1.0" diameter="1.8"/>

<pad name="DOUT" x="2.54" y="0.00" drill="1.0" diameter="1.8"/>

<!-- Silk -->
<wire x1="-1.5" y1="4.0" x2="4.0" y2="4.0" width="0.127" layer="21"/>
<wire x1="4.0" y1="4.0" x2="4.0" y2="-4.0" width="0.127" layer="21"/>
<wire x1="4.0" y1="-4.0" x2="-1.5" y2="-4.0" width="0.127" layer="21"/>
<wire x1="-1.5" y1="-4.0" x2="-1.5" y2="4.0" width="0.127" layer="21"/>

<text x="-3.0" y="2.1" size="1.27" layer="21">5V</text>
<text x="-3.0" y="-0.4" size="1.27" layer="21">DIN</text>
<text x="-3.0" y="-2.9" size="1.27" layer="21">GND</text>
<text x="3.3"  y="-0.4" size="1.27" layer="21">DO</text>

<text x="-4.5" y="4.8" size="1.27" layer="25">&gt;NAME</text>
<text x="-4.5" y="-6.4" size="1.27" layer="27">&gt;VALUE</text>
</package>
</packages>

<!-- ================================================= -->
<!-- SYMBOL -->
<!-- ================================================= -->
<symbols>
<symbol name="WS2812_SINGLE">
<description>Single WS2812 / NeoPixel</description>

<wire x1="-10.16" y1="-7.62" x2="10.16" y2="-7.62" width="0.254" layer="94"/>
<wire x1="10.16" y1="-7.62" x2="10.16" y2="7.62" width="0.254" layer="94"/>
<wire x1="10.16" y1="7.62" x2="-10.16" y2="7.62" width="0.254" layer="94"/>
<wire x1="-10.16" y1="7.62" x2="-10.16" y2="-7.62" width="0.254" layer="94"/>

<text x="-10.16" y="8.89" size="1.778" layer="95">&gt;NAME</text>
<text x="-10.16" y="-10.16" size="1.778" layer="96">&gt;VALUE</text>

<pin name="5V"  x="-15.24" y="2.54" length="short" direction="pwr"/>
<pin name="DIN" x="-15.24" y="0.00" length="short" direction="in"/>
<pin name="GND" x="-15.24" y="-2.54" length="short" direction="pwr"/>

<pin name="DOUT" x="15.24" y="0.00" length="short" direction="out" rot="R180"/>

<text x="-7.62" y="4.06" size="1.27" layer="94">WS2812</text>
</symbol>
</symbols>

<!-- ================================================= -->
<!-- DEVICE -->
<!-- ================================================= -->
<devicesets>
<deviceset name="WS2812_SINGLE_THT" prefix="LED">
<description>Single WS2812 / NeoPixel – THT 2.54mm pads</description>

<gates>
<gate name="G$1" symbol="WS2812_SINGLE" x="0" y="0"/>
</gates>

<devices>
<device name="" package="WS2812_THT_2P54">
<connects>
<connect gate="G$1" pin="5V"  pad="5V"/>
<connect gate="G$1" pin="DIN" pad="DIN"/>
<connect gate="G$1" pin="GND" pad="GND"/>
<connect gate="G$1" pin="DOUT" pad="DOUT"/>
</connects>

<technologies>
<technology name=""/>
</technologies>
</device>
</devices>
</deviceset>
</devicesets>

</library>
</drawing>
</eagle>
