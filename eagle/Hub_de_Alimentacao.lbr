<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE eagle SYSTEM "eagle.dtd">
<eagle version="9.6.2">
<drawing>
<settings>
<setting alwaysvectorfont="yes"/>
<setting verticaltext="up"/>
</settings>

<library>
<description>
5V Power Hub: 1x IN (5V,GND) + 3x OUT (5V,GND) for LED strips
THT 2.54mm connectors
</description>

<packages>
  <package name="PWR_HUB_1IN_3OUT_THT_2P54">
    <description>
Footprint: 4x 2-pin THT headers (2.54mm)
JIN at left, JOUT1..3 at right
Pin1=5V, Pin2=GND
    </description>

    <!-- Simple outline (silk) -->
    <wire x1="-12" y1="-10" x2="12" y2="-10" width="0.127" layer="21"/>
    <wire x1="12" y1="-10" x2="12" y2="10" width="0.127" layer="21"/>
    <wire x1="12" y1="10" x2="-12" y2="10" width="0.127" layer="21"/>
    <wire x1="-12" y1="10" x2="-12" y2="-10" width="0.127" layer="21"/>

    <!-- IN connector (left) -->
    <pad name="IN_5V"  x="-8.89" y="1.27" drill="1.0" diameter="1.8" shape="square"/>
    <pad name="IN_GND" x="-8.89" y="-1.27" drill="1.0" diameter="1.8"/>

    <!-- OUT1 (right, top) -->
    <pad name="O1_5V"  x="8.89" y="6.35" drill="1.0" diameter="1.8" shape="square"/>
    <pad name="O1_GND" x="8.89" y="3.81" drill="1.0" diameter="1.8"/>

    <!-- OUT2 (right, middle) -->
    <pad name="O2_5V"  x="8.89" y="1.27" drill="1.0" diameter="1.8" shape="square"/>
    <pad name="O2_GND" x="8.89" y="-1.27" drill="1.0" diameter="1.8"/>

    <!-- OUT3 (right, bottom) -->
    <pad name="O3_5V"  x="8.89" y="-3.81" drill="1.0" diameter="1.8" shape="square"/>
    <pad name="O3_GND" x="8.89" y="-6.35" drill="1.0" diameter="1.8"/>

    <!-- Labels -->
    <text x="-11.5" y="8.5" size="1.27" layer="21">IN</text>
    <text x="7.0" y="8.5" size="1.27" layer="21">OUT1</text>
    <text x="7.0" y="3.0" size="1.27" layer="21">OUT2</text>
    <text x="7.0" y="-2.5" size="1.27" layer="21">OUT3</text>

    <text x="-12" y="11.5" size="1.27" layer="25">&gt;NAME</text>
    <text x="-12" y="-13.5" size="1.27" layer="27">&gt;VALUE</text>
  </package>
</packages>

<symbols>
  <symbol name="PWR_HUB_5V_1IN_3OUT">
    <description>5V power distribution hub: 1 input, 3 outputs</description>

    <wire x1="-15.24" y1="-10.16" x2="15.24" y2="-10.16" width="0.254" layer="94"/>
    <wire x1="15.24" y1="-10.16" x2="15.24" y2="10.16" width="0.254" layer="94"/>
    <wire x1="15.24" y1="10.16" x2="-15.24" y2="10.16" width="0.254" layer="94"/>
    <wire x1="-15.24" y1="10.16" x2="-15.24" y2="-10.16" width="0.254" layer="94"/>

    <text x="-15.24" y="11.43" size="1.778" layer="95">&gt;NAME</text>
    <text x="-15.24" y="-12.70" size="1.778" layer="96">&gt;VALUE</text>

    <!-- Input -->
    <pin name="5V_IN"  x="-20.32" y="5.08" length="short" direction="pwr"/>
    <pin name="GND_IN" x="-20.32" y="2.54" length="short" direction="pwr"/>

    <!-- Outputs -->
    <pin name="5V_OUT1"  x="20.32" y="7.62" length="short" direction="pwr" rot="R180"/>
    <pin name="GND_OUT1" x="20.32" y="5.08" length="short" direction="pwr" rot="R180"/>

    <pin name="5V_OUT2"  x="20.32" y="0.00" length="short" direction="pwr" rot="R180"/>
    <pin name="GND_OUT2" x="20.32" y="-2.54" length="short" direction="pwr" rot="R180"/>

    <pin name="5V_OUT3"  x="20.32" y="-7.62" length="short" direction="pwr" rot="R180"/>
    <pin name="GND_OUT3" x="20.32" y="-10.16" length="short" direction="pwr" rot="R180"/>

    <text x="-10.16" y="-1.27" size="1.27" layer="94">5V POWER HUB</text>
  </symbol>
</symbols>

<devicesets>
  <deviceset name="PWR_HUB_5V_1IN_3OUT" prefix="J">
    <description>5V Power Hub: 1 input + 3 outputs (THT 2.54mm)</description>

    <gates>
      <gate name="G$1" symbol="PWR_HUB_5V_1IN_3OUT" x="0" y="0"/>
    </gates>

    <devices>
      <device name="" package="PWR_HUB_1IN_3OUT_THT_2P54">
        <connects>
          <connect gate="G$1" pin="5V_IN"    pad="IN_5V"/>
          <connect gate="G$1" pin="GND_IN"   pad="IN_GND"/>

          <connect gate="G$1" pin="5V_OUT1"  pad="O1_5V"/>
          <connect gate="G$1" pin="GND_OUT1" pad="O1_GND"/>

          <connect gate="G$1" pin="5V_OUT2"  pad="O2_5V"/>
          <connect gate="G$1" pin="GND_OUT2" pad="O2_GND"/>

          <connect gate="G$1" pin="5V_OUT3"  pad="O3_5V"/>
          <connect gate="G$1" pin="GND_OUT3" pad="O3_GND"/>
        </connects>

        <technologies>
          <technology name=""/>
        </technologies>
      </device>
    </devices>
  </deviceset>
</devicesets>

</library>
</drawing>
</eagle>
